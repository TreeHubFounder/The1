
# Production Environment Variables for treehub.app
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://treehub.app
NEXT_PUBLIC_DOMAIN=treehub.app

# NextAuth Configuration
NEXTAUTH_URL=https://treehub.app
NEXTAUTH_SECRET=treehub_production_secret_key_2024_secure

# Database (will be configured during deployment)
DATABASE_URL=postgresql://treehub_user:secure_password@db:5432/treehub_production

# Application Features
ENABLE_MARKET_CONQUEST=true
ENABLE_AI_AGENTS=true
ENABLE_WEATHER_MONITORING=true
ENABLE_ANALYTICS=true

# Security Configuration
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100
LOG_LEVEL=error

# Email Configuration
SMTP_HOST=smtp.treehub.app
SMTP_PORT=587
SMTP_USER=noreply@treehub.app

# Performance & Monitoring
ENABLE_COMPRESSION=true
ENABLE_CACHING=true
